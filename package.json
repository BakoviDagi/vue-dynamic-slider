  "files": [
    "dist/*",
    "src/*",
    "!/**/demo",
    "!dist/demo.html"
  ],
  "peerDependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^8.7.1",
    "sass": "^1.55.0",
    "vue": "^3.2.41",
    "vite": "^3.2.0"
  },
  "bugs": {
    "url": "https://github.com/BakoviDagi/vue-dynamic-slider/issues"
  },
  "homepage": "https://github.com/BakoviDagi/vue-dynamic-slider",
  "keywords": [
    "vue",
    "slider",
    "vue-slider",
    "carousel",
    "vue-carousel",
    "vue-dynamic-slider"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BakoviDagi/vue-dynamic-slider.git"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2021": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}

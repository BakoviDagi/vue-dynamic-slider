{
  "name": "vue-dynamic-slider",
  "version": "0.5.1",
  "description": "A light-weight slider for Vue",
  "author": "BakoviDagi",
  "scripts": {
    "dev": "vite serve demo",
    "build": "vite build",
    "lint": "vue-cli-service lint src/index.js"
  },
  "type": "module",
  "main": "./dist/vue-dynamic-slider.umd.cjs",
  "module": "./dist/vue-dynamic-slider.js",
  "exports": {
    ".": {
      "import": "./dist/vue-dynamic-slider.js",
      "require": "./dist/vue-dynamic-slider.umd.cjs"
    }
